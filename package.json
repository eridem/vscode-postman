{
    "name": "vscode-postman",
    "displayName": "Postman Runner",
    "description": "Run Postman tests directly from your VSCode",
    "version": "0.0.4",
    "publisher": "eridem",
    "author": {
        "name": "Miguel Ángel Domínguez Coloma",
        "email": "m@eridem.net",
        "url": "http://eridem.net"
    },
    "contributors": [],
    "license": "MIT",
    "homepage": "http://vscode-postman.eridem.net",
    "icon": "icon.svg",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "bugs": {
        "url": "https://github.com/eridem/vscode-postman/issues",
        "email": "m@eridem.net"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:eridem/vscode-postman.git"
    },
    "activationEvents": [
        "onCommand:extension.question-mode"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.question-mode",
                "title": "Postman > Run > Question Mode"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "newman": "^3.2.0"
    }
}